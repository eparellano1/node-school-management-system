{
    "version": 2,
    "buildCommand": "yarn server",
    "installCommand": "npm install",
    "builds": [
        { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [{ 
        "src": "/(.*)", 
        "dest": "/",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    }] 
}